class Main
{
	static void run()
	{
		String str = "Abcdefghijklmnoprstuvwxyz";
		ifj16.print("str = "+str+"\n");

		String substr = ifj16.substr(str, 5, 4);
		ifj16.print("substr = substr(str, 5, 4) = " + substr + "\n");

		int i = ifj16.compare(str,substr);
		ifj16.print("i = compare(str,substr) = " + i + "\n");

		double j = ifj16.compare("fghi",substr);
		ifj16.print("j = compare(\"fghi\",substr) = " + j + "\n");

		int a = ifj16.length(str);
		ifj16.print("a = length(str) = " + a + "\n");

		double b = ifj16.length(substr);
		ifj16.print("b = length(substr) = " + b + "\n");

		double c = ifj16.length("fghi");
		ifj16.print("c = length(\"fghi\") = " + c + "\n");
	}
}