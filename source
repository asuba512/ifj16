/* Program 3: Prace s retezci a vestavenymi funkcemi */
class Main
{
static int x;
static void run()
{
String str1;
String str2;
Main.x = ifj16.find(str2, "text");
ifj16.print();
Game.play(str1);
}
// end of static void run()
}
// end of class Main
class Game
{
