str = ifj16.readString();
str = ifj16.sort(str);
int cmp = ifj16.compare(str, "abcdefgh");
if () {
while () {
ifj16.print("Spatne zadana posloupnost, zkuste znovu:");
str = ifj16.readString();
str = ifj16.sort(str);
cmp = ifj16.compare(str, "abcdefgh");
}
}
else {
}
return;
}
// end of static void play(String)
}
// end of class Game
